{
  "version": 1,
  "paths": {
    "/articles": {
      "name": "/articles",
      "lambdaFunction": "getArticle",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/sign-up": {
      "name": "/sign-up",
      "lambdaFunction": "getArticle",
      "permissions": {
        "setting": "open"
      }
    }
  }
}